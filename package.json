{
  "name": "@scaffoldly/openapi-generator",
  "version": "1.0.27-0",
  "description": "Wrapper for OpenAPITools/openapi-generator-cli",
  "main": "dist/index.js",
  "bin": {
    "openapi-generator": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --trace-warnings -r ts-node/register -r tsconfig-paths/register index.ts",
    "prepare": "yarn build",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix && yarn run prettier --write 'src/**/*.{js,ts,tsx}'"
  },
  "files": [
    "dist/index.js"
  ],
  "repository": "git@github.com:scaffoldly/openapi-generator",
  "author": "Christian Nuss",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "axios": "^0.21.1",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "p-queue": "6.6.2",
    "simple-git": "^2.39.0",
    "uri-js": "^4.4.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@types/dns-packet": "^5.2.2",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.4",
    "@types/p-queue": "^3.2.1",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "14.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "cli",
    "scaffoldly",
    "openapi",
    "generator"
  ]
}